mpg_subset <- reactive({
mtcars %>%
filter(
hp >= input$hp[1],
hp <= input$hp[2],
cyl %in% input$cyl
)
})
renderPlot({
ggplot(mpg_subset(), aes(x = wt, y = mpg, color = factor(cyl))) +
geom_point()
})
